<Window x:Class="Final_Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Final_Project"
        mc:Ignorable="d"
        Title="Password Brute Force" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Button Content="Encrypt and Save Password" HorizontalAlignment="Left" Margin="30,42,0,0" VerticalAlignment="Top" Click="EncryptButton_Click" Height="20" Width="147"/>
        <Button Content="Brute force attack" HorizontalAlignment="Left" Margin="55,102,0,0" VerticalAlignment="Top" Height="20" Width="97" Click="BruteForceButton_Click"/>
        <TextBox x:Name="PasswordTextBox" HorizontalAlignment="Left" Margin="226,42,0,0" TextWrapping="Wrap" Text="Enter password..." VerticalAlignment="Top" Width="174" Height="20"/>
        <TextBlock x:Name="ResultTextBlock" HorizontalAlignment="Center" Margin="0,102,0,0" TextWrapping="Wrap" Text="Results:" VerticalAlignment="Top" Height="45" Width="396"/>

    </Grid>
</Window>
